#!/usr/bin/env ruby

require 'edifact_converter'
EdifactConverter::Configuration.load

EdifactConverter::Configuration.hide_position = true
result = EdifactConverter.convert_edifact(EdifactConverter.read_file('test/files/sygehushenv_ver30.edi'))
p result.messages

result = EdifactConverter.convert_xml(result.xml.to_s)
p result.messages